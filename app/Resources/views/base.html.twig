<html>
    <head>
        <meta charset="UTF-8" />
        <title>{% block title %}Test App{% endblock %}</title>
        {% block stylesheets %}
            {% stylesheets '@AppBundle/Resources/public/bootstrap/css/bootstrap.css' %}
                <link rel="stylesheet" href="{{ asset_url }}" />
            {% endstylesheets %}
        {% endblock %}
        <link rel="stylesheet" href="bootstrap/css/bootstrap.css" />
        <link rel="stylesheet" href="fancygrid/fancy.min.css">
        <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />
    </head>
    <body>
        {% block body %}{% endblock %}
        {% block javascripts %}
            {% javascripts '@AppBundle/Resources/public/js/jquery-3.1.1.min.js' '@AppBundle/Resources/public/bootstrap/js/bootstrap.min.js' '@AppBundle/Resources/public/js/main.js' '@AppBundle/Resources/public/fancygrid/fancy.min.js'%}
                <script type="text/javascript" src="{{ asset_url }}"></script>
            {% endjavascripts %}
        {% endblock %}
        
    </body>
</html>
